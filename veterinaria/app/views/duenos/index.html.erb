<h1>Listing duenos</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @duenos.each do |dueno| %>
  <tr>
    <td><%= dueno.nombre %></td>
    <td><%= link_to 'Show', dueno %></td>
    <td><%= link_to 'Edit', edit_dueno_path(dueno) %></td>
    <td><%= link_to 'Destroy', dueno, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

